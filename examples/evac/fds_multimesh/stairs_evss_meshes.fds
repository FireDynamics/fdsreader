&EVAC ID                      = "Students",
      NUMBER_INITIAL_PERSONS  =  100,
      XB                      = 36.5, 46.5,-5.0, 5.0, 3.8, 4.6,
      AVATAR_COLOR            = "BLUE",
      PERS_ID                 = "Students" /

&EVAC ID                      = "Children",
      NUMBER_INITIAL_PERSONS  =  100,
      XB                      = 36.5, 46.5,-5.0, 5.0, 3.8, 4.6,
      AVATAR_COLOR            = "BLUE",
      PERS_ID                 = "School children" /

