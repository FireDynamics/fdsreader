/ -------------------------------------------------------------------
&HEAD CHID  = "stairs_evss_meshes",
      TITLE = "case"  /

/ -------------------------------------------------------------------
/ Mesh - fire
/ -------------------------------------------------------------------

/ -------------------------------------------------------------------
/ Mesh - evac
/ -------------------------------------------------------------------
&MESH IJK     = 165,100, 1,
      ID      = "GroundFloor",
      XB      = 0.0,16.5,-5.0,  5.0, 0.8, 1.6,
      EVACUATION      = .TRUE.,
      EVAC_HUMANS     = .TRUE.,
      EVAC_Z_OFFSET   = 1.0 /

&MESH IJK     = 300,100, 1,
      ID      = "FirstFloor",
      XB      =16.5,46.5,-5.0,  5.0, 3.8, 4.6,
      EVACUATION      = .TRUE.,
      EVAC_HUMANS     = .TRUE.,
      EVAC_Z_OFFSET   = 1.0 /

/ -------------------------------------------------------------------
/ miscellaneous
/ -------------------------------------------------------------------
&MISC NO_EVACUATION             = .FALSE.,
      EVACUATION_INITIALIZATION = .FALSE.,
      EVACUATION_MC_MODE        = .TRUE.,
      EVACUATION_DRILL          = .FALSE.,
      EVACUATION_WRITE_FED      = .FALSE.,
      EVAC_TIME_ITERATIONS      = 50,
      EVAC_PRESSURE_ITERATIONS  = 50 /

&DUMP DT_PART = 0.5,
      DT_HRR  = 0.1,
      DT_SLCF = 1000000.0,
      DT_PL3D = 1000000.0,
      DT_ISOF = 1000000.0 /

/ -------------------------------------------------------------------
/ simulation time
/ -------------------------------------------------------------------
&TIME T_BEGIN           =   0.0,
      T_END             = 600.0,
      EVAC_DT_FLOWFIELD =   0.01 /

/ -------------------------------------------------------------------
/ Evac - EXIT
/ -------------------------------------------------------------------
&EXIT ID         = "EXIT",
      IOR        = -1,
      COLOR      = "GREEN",
      COUNT_ONLY = .FALSE.,
      KNOWN_DOOR = .TRUE.,
      XYZ        =  0.2, 0.0, 2.0
      XB         =  0.0, 0.0, -3.0, 3.0, 0.8, 1.6 /

/ -------------------------------------------------------------------
/ Evac - stair
/ -------------------------------------------------------------------

&DOOR ID         = 'T01',
      IOR        = -1,
      EXIT_SIGN  = .TRUE.,
      COLOR      = 'ORANGE',
      KNOWN_DOOR = .TRUE.,
      XYZ        =16.7, 0.0, 2.0,
      XB         =16.5,16.5,-1.2, 1.2, 3.8, 4.6,
      TO_NODE    = 'E00' /

&ENTR ID         = 'E00',
      XB         =16.5,16.5,-1.2, 1.2, 0.8, 1.6,
      IOR        = -1,
      COLOR      = 'ORANGE' /

&EVSS ID                     = 'stair',
      IOR                    = -1,
      SHOW                   = .TRUE.,
      XB                     = 8.0,16.5,-1.2, 1.2, 0.8, 1.6,
      FAC_V0_UP              = 0.5,
      FAC_V0_DOWN            = 0.5,
      FAC_V0_HORI            = 1.0,
      HEIGHT0                = 0.0,
      HEIGHT                 = 3.0,
      MESH_ID                = 'GroundFloor' /

/ -------------------------------------------------------------------
/ EVAC - counter
/ -------------------------------------------------------------------
&EXIT ID         = 'C1',
      IOR        = -1,
      COUNT_ONLY = .TRUE.,
      XYZ        = 6.2, 0.0, 2.0,
      XB         = 6.0, 6.0,-0.7, 0.7, 0.8, 1.6 /

&EXIT ID         = 'C2',
      IOR        = -1,
      COUNT_ONLY = .TRUE.,
      XYZ        = 8.2, 0.0, 2.0,
      XB         = 8.0, 8.0,-1.2, 1.2, 0.8, 1.6 /

&EXIT ID         = 'C3',
      IOR        = -1,
      COUNT_ONLY = .TRUE.,
      XYZ        =16.7, 0.0, 2.0,
      XB         =16.5,16.5,-1.2, 1.2, 3.8, 4.6 /

&EXIT ID         = 'C4',
      IOR        = -1,
      COUNT_ONLY = .TRUE.,
      XYZ        =36.7, 0.0, 2.0,
      XB         =36.5,36.5,-1.2, 1.2, 3.8, 4.6 /

/ -------------------------------------------------------------------
/ EVAC - PERS
/ -------------------------------------------------------------------
&PERS ID                   = "ADULTS",
      DEFAULT_PROPERTIES   = "Adult",
      PRE_EVAC_DIST   = 0,
          PRE_MEAN    = 0.0,
      DET_EVAC_DIST   = 0,
          DET_MEAN    = 0.0,
      VELOCITY_DIST   = 0,
