      DENS_INIT       = 6.0,
      OUTPUT_FED           = .FALSE.,
      OUTPUT_SPEED         = .TRUE.,
      OUTPUT_CONTACT_FORCE = .FALSE.,
      OUTPUT_TOTAL_FORCE   = .FALSE.,
      OUTPUT_DENSITY       = .TRUE.,
      OUTPUT_NERVOUSNESS   = .FALSE.,
      OUTPUT_ACCELERATION  = .FALSE.,
