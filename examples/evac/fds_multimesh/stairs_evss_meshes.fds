           VEL_HIGH        = 1.3,
           VEL_PARA        = 0.30,
       DET_EVAC_DIST       = 0,
            DET_MEAN       = 0.00,
      PRE_EVAC_DIST        = 0,
           PRE_MEAN        = 0.00,
      SMOKE_MIN_SPEED      = 0.10,
      DENS_INIT            = 6.00,
      HUMAN_SMOKE_HEIGHT   = 1.80,
      TDET_SMOKE_DENS      = 0.00,
      COLOR_METHOD         = 4 /


