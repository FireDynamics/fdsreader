/ -------------------------------------------------------------------
&HEAD   CHID    ='fdsreader_slcfs',
        TITLE   ='' /
/ -------------------------------------------------------------------
/ Mesh
/ -------------------------------------------------------------------
/ Mesh size - dx = 0.1
&MESH   ID      ='MESH_1',
        IJK     = 10, 10, 10,
        XB      = 0.0, 1.0, 0.0, 1.0, 0.0, 1.0 /
&MESH   ID      ='MESH_2',
        IJK     = 10, 10, 10,
        XB      = 0.5, 1.5, 1.0, 2.0, 0.0, 1.0 /
&MESH   ID      ='MESH_3',
        IJK     = 20, 10, 10,
        XB      = 1.0, 3.0, 0.0, 1.0, 0.0, 1.0 /
&MESH   ID      ='MESH_4',
        IJK     = 20, 10, 10,
        XB      = 0.0, 2.0,-1.0, 0.0, 0.0, 1.0 /

/ -------------------------------------------------------------------
/ simulation time
/ -------------------------------------------------------------------
&TIME   T_BEGIN =   0.0,
        T_END   = 120.0 /
/ -------------------------------------------------------------------
/ boundary conditions
/ -------------------------------------------------------------------
&VENT   MB ='XMIN', SURF_ID='OPEN' /
&VENT   MB ='XMAX', SURF_ID='OPEN' /
&VENT   MB ='YMIN', SURF_ID='OPEN' /
&VENT   MB ='YMAX', SURF_ID='OPEN' /
VENT   MB ='ZMIN', SURF_ID='OPEN' /
&VENT   MB ='ZMAX', SURF_ID='OPEN' /

&DUMP   DT_HRR      =  1.0,
        DT_SLCF     = 10.0,
        DT_DEVC     = 10.0,
        DT_CTRL     = 10.0,
        DT_PART     = 10.0,
        DT_BNDF     = 30.0,
        DT_SMOKE3D  =  1.0,
        VELOCITY_ERROR_FILE = .FALSE.,
        MASS_FILE           = .FALSE. /

/ -------------------------------------------------------------------
/ fire - reaction
/ -------------------------------------------------------------------
&REAC ID                 = 'METHANE' /

/ fire - area
/ -------------------------------------------------------------------
&SURF ID        = 'fire',
      COLOR     = 'RED',
      HRRPUA    =  300.0,
      RAMP_Q    = 'fire-ramp' /

&RAMP ID='fire-ramp', T=    0.00, F=0.00 /
&RAMP ID='fire-ramp', T=   30.00, F=1.00 /

&OBST XB = 0.6, 0.7, 0.6, 0.7, 0.0, 0.0, SURF_ID='fire' /

/ -------------------------------------------------------------------
/ quantities
/ -------------------------------------------------------------------
/ Animated Planar Slices - SLCF
/ -----------------------------
&SLCF PBZ= 0.5, QUANTITY='TEMPERATURE' /

&TAIL /
